<?xml version="1.0" encoding="UTF-8"?>

<injections>

	<!--TeX: gets direct injected (see ConTeXtSyntax.xml) - syntax of ConTeXt/TeX are nested to complex to make simple injections here-->
	<!--Lua-->
	<injection name="injection.LuaTeX-ConTeXt" selector="language-root.context container.language-root.lua" action="insert-before-children">
		<include syntax="language-root.LuaTeX"/>
	</injection>
	<!--MetaFun-->
    <injection name="injection.MetaFun-ConTeXt" selector="language-root.context container.language-root.metafun" action="insert-before-children">
    	<include syntax="language-root.MetaFun"/>
    </injection>
      
</injections>