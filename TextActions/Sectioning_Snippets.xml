<?xml version="1.0" encoding="UTF-8"?>

<action-recipes>

    <snippet id="actions.snippets.context.sectioning.startpart" category="actions.snippets.context/actions.snippets.context.sectioning">
        <title>start new part</title>
        <text>\\startpart[title={${1:placeholder}},
               bookmark={${2:$1}},
               list={${3:$1}},
               marking={${4:$1}},
               reference={${5:$1}}
        	  ]
        		  
$6
        
\\stoppart</text>
        <syntax-context>structural.container.document,structural.container.document *</syntax-context>
        <text-trigger>par</text-trigger>
    </snippet>
    
    <snippet id="actions.snippets.context.sectioning.startchapter" category="actions.snippets.context/actions.snippets.context.sectioning">
